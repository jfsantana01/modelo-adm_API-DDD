{
  "name": "modelo-adm_api_solid",
  "version": "1.0.52",
  "description": "modelo de api administrativa com typeorm e solid",
  "scripts": {
    "start:debug": "tsc --noEmit && node --nolazy --inspect-brk ./dist/shared/infra/http/server.js",
    "start": "tsc --noEmit && ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts ",
    "start:sync": "tsc --noEmit && ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts sync",
    "build": "tsc --noEmit && babel src --extensions \".ts,.js\" --out-dir dist --copy-files --no-comments --source-maps inline",
    "copy-files": "npx copyfiles package.json dist",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/shared/infra/typeorm/index.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/shared/infra/typeorm/index.ts",
    "migration:show": "npm run typeorm migration:show  -- -d ./src/shared/infra/typeorm/index.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/shared/infra/typeorm/index.ts -- src/shared/infra/typeorm/migrations",
    "migration:create": "node services/typeorm/createMigration",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "jest",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "contabo1": "node deploy/enviarServidor.js contabo1",
    "contabo1hml": "node deploy/enviarServidor.js contabo1hml"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.3",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "fs-extra": "^11.2.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "mime": "^4.0.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "rate-limiter-flexible": "^5.0.3",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "ssh2-sftp-client": "^11.0.0",
    "tsx": "^4.19.1",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.10.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.5",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "ncp": "^2.0.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0"
  }
}
